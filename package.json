{
  "name": "image_processing_api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "yarn build && node ./dist/index.js",
    "prebuild": "rimraf dist",
    "start-dev": "tsc && nodemon --watch src src/index.ts -r ts-node/register --inspect",
    "build": "yarn prebuild && npx tsc -p .",
    "dev-up": "db-migrate up -m ./_database/migrations  --config ./_database/config/dev.json",
    "dev-down": "db-migrate reset -m ./_database/migrations --config ./_database/config/dev.json",
    "test-up": "db-migrate up -e test -m ./_database/migrations  --config ./_database/config/test.json ",
    "test-down": "db-migrate reset  -e test -m ./_database/migrations --config ./_database/config/test.json",
    "reset-test-db": "db-migrate reset -e test",
    "test": "npm run build && npm run jasmine && yarn reset-test-db && db-migrate up -e test && NODE_ENV=test",
    "prettier": "lint-staged",
    "jasmine": "jasmine",
    "lint": "eslint  . --ext .ts"
  },
  "lint-staged": {
    "src/**/*.{js,json,ts}": "prettier --single-quote --trailing-comma all --tab-width 4 --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "duyphuongls1999",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jasmine": "^5.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.1",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/db-migrate-pg": "^0.0.10",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.3.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.2.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  }
}
